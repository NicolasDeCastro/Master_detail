<mvc:View controllerName="nico.app.testenovoapp.controller.S1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
            <Page title="{i18n>title}">
                <content>
                <List items="{/Products}" itemPress="onItemPress">
                
                    <items>
                        <ObjectListItem type="Active" intro="{ProductID}"  title="{ProductName}" number="{UnitPrice}" numberUnit="reais">
                            <attributes>
                            
                                <ObjectAttribute title="em estoque" text="{UnitsInStock}"></ObjectAttribute>
                            
                            </attributes>
                        
                        </ObjectListItem>
                    
                    
                    
                    
                    
                    </items>
                
                
                
                
                </List>
                
                
                
                
                
                
                
                </content>
            </Page>
    
</mvc:View>
